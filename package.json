{
  "name": "stefanicjuraj-github-io",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "node node_modules/@docusaurus/core/bin/docusaurus.mjs",
    "start": "node node_modules/@docusaurus/core/bin/docusaurus.mjs start",
    "build": "node node_modules/@docusaurus/core/bin/docusaurus.mjs build",
    "swizzle": "node node_modules/@docusaurus/core/bin/docusaurus.mjs swizzle",
    "deploy": "node node_modules/@docusaurus/core/bin/docusaurus.mjs deploy",
    "deploy-manual": "npm run build && rm -rf /tmp/site-build && cp -R build /tmp/site-build && git checkout gh-pages && rm -rf * && cp -R /tmp/site-build/* . && touch .nojekyll && git add . && git commit -m \"build: deploy\" && git push origin gh-pages && git checkout main",
    "clear": "node node_modules/@docusaurus/core/bin/docusaurus.mjs clear",
    "serve": "node node_modules/@docusaurus/core/bin/docusaurus.mjs serve",
    "write-translations": "node node_modules/@docusaurus/core/bin/docusaurus.mjs write-translations",
    "write-heading-ids": "node node_modules/@docusaurus/core/bin/docusaurus.mjs write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@easyops-cn/docusaurus-search-local": "^0.52.2",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.2",
    "@docusaurus/tsconfig": "3.9.2",
    "@docusaurus/types": "3.9.2",
    "gh-pages": "^6.1.1",
    "typescript": "~5.5.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
